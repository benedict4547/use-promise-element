{
  "name": "use-node-promise",
  "version": "0.0.1",
  "description": "React hook 'useNodePromise'",
  "main": "./dist",
  "typings": "./typings",
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "minify": "./node_modules/.bin/minify dist/index.js --out-file dist/index.js",
    "postbuild": "yarn minify",
    "build": "./node_modules/.bin/tsc -p tsconfig.build.json",
    "rebuild": "rm -fr dist typings && yarn build",
    "link:pack": "sh ./scripts/link-pack.sh",
    "unlink:pack": "sh ./scripts/unlink-pack.sh",
    "prepack": "yarn rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EvgenyiFedotov/use-node-promise.git"
  },
  "keywords": [
    "react",
    "hook",
    "promise",
    "node"
  ],
  "files": [
    "dist",
    "typings"
  ],
  "author": "e.fedotov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EvgenyiFedotov/use-node-promise/issues"
  },
  "homepage": "https://github.com/EvgenyiFedotov/use-node-promise#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.13",
    "babel-minify": "^0.5.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.7.1",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.2"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  }
}
